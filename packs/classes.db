{"name":"Deed: Steady Aim","type":"feat","img":"modules/dark-matter-compendium/images/icons/black/smoking-finger.svg","effects":[],"flags":{"betterRolls5e":{"critRange":{"value":null,"type":"String"},"critDamage":{"value":"","type":"String"},"quickDesc":{"value":true,"type":"Boolean","altValue":true},"quickProperties":{"value":true,"type":"Boolean","altValue":true},"quickOther":{"value":true,"context":"+AC","type":"Boolean","altValue":true},"quickFlavor":{"value":true,"type":"Boolean","altValue":true},"quickCharges":{"value":{"use":true,"resource":true,"charge":true},"type":"Boolean","altValue":{"use":true,"resource":true,"charge":true}},"quickDamage":{"context":[],"value":[],"type":"Array","altValue":[]},"quickSave":{"value":true,"type":"Boolean","altValue":true},"quickAttack":{"type":"Boolean","value":true,"altValue":true},"quickTemplate":{"type":"Boolean","value":true,"altValue":true},"quickPrompt":{"type":"Boolean","value":false,"altValue":false}},"core":{"sourceId":"Item.I8oOp2crZkMlVwfJ"},"cf":{"id":"temp_iiwyyuu57n9"}},"system":{"description":{"value":"<p><strong>Steady Aim. </strong>On your turn, you can use a bonus action and expend one risk die to double the normal and maximum range for the next ranged weapon attack you make.</p>\n<section class=\"secret\">\n<p>(Set Resource Consumption to Item Uses &gt; Risk Dice)</p>\n</section>","chat":"","unidentified":""},"source":"Mage Hand Press","activation":{"type":"bonus","cost":1,"condition":""},"duration":{"value":null,"units":""},"target":{"value":null,"width":null,"units":"","type":"self"},"range":{"value":null,"long":null,"units":""},"uses":{"value":0,"max":"0","per":"","recovery":""},"consume":{"type":"charges","target":"","amount":null},"ability":"","actionType":"","attackBonus":0,"chatFlavor":"Double Min/Max Weapon Range","critical":null,"damage":{"parts":[],"versatile":""},"formula":"1d8","save":{"ability":"con","dc":null,"scaling":"con"},"requirements":"Gunslinger (2)","recharge":{"value":null,"charged"